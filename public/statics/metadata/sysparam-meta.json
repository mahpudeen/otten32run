{
    "title": "TEKNO",
    "multiselect": true,
    "idColumn": "id",
    "isEmbeddedId": true,
    "action": {
        "add": true,
        "edit": true,
        "save": true,
        "delete": true,
        "view": true
    },    
    "rows": [
        10,
        20,
        30,
        40,
        50         
    ],
    "export": {
        "pdf": true,
        "copy": true,
        "csv": true,
        "excel": true,
        "print": true
    },
    "table": {
        "items": [
            {
                "header": "Kode Sistem",
                "column": "id.systemCode",
                "align": {
                    "header": "center",
                    "column": "left"
                }
            },
            {
                "header": "Kode Parameter",
                "column": "id.paramCode",
                "align": {
                    "header": "center",
                    "column": "left"
                }
            },
            {
                "header": "Nilai",
                "column": "value",
                "align": {
                    "header": "center",
                    "column": "left"
                }
            },
            {
                "header": "Deskripsi",
                "column": "description",
                "align": {
                    "header": "center",
                    "column": "left"
                }
            },
            {
                "header": "Organisasi",
                "column": "${organisation.name} - ${organisation.group}",
                "align": {
                    "header": "center",
                    "column": "left"
                }
            }
        ],
        "footer": false
    },
    "order": {
        "type": "single",
        "items": [
            {
             "label": "Kode Sistem",
             "column": "id.systemCode",
             "desc": true
            },
            {
             "label": "Kode Parameter",
             "column": "id.paramCode",
             "desc": false
            }
        ]
    },
    "filter": {
        "type": "single",
        "column": 2,
        "items": [
            {
                "label": "Kode Sistem",
                "column": "id.systemCode",
                "condition": "anylike",
                "and": true
            },
            {
                "label": "Kode Parameter",
                "column": "id.paramCode",
                "condition": "eq",
                "and": true
            }
        ]
    },    
    "columns": {
        "id.systemCode": {
            "label": "Kode Sistem",
            "controlType": "text",
            "align": "left",
            "editable": true,
            "insertable": true,
            "visible": true,
            "readOnly": true,
            "required": true,
            "defaultValue": "",
            "order": 0
        },
        "id.paramCode": {
            "label": "Kode Parameter",
            "controlType": "text",
            "align": "left",
            "editable": true,
            "insertable": true,
            "visible": true,
            "readOnly": true,
            "required": true,
            "defaultValue": "",
            "order": 1
        },
        "value": {
            "label": "Nilai",
            "controlType": "textarea",
            "align": "left",
            "editable": true,
            "insertable": true,
            "visible": true,
            "required": true,
            "defaultValue": "",
            "order": 2
        },
        "description": {
            "label": "Deskripsi",
            "controlType": "textarea",
            "align": "left",
            "editable": true,
            "insertable": true,
            "visible": true,
            "required": true,
            "defaultValue": "",
            "order": 3
        },
        "organisation": {
            "label": "Organisasi",
            "controlType": "text",
            "align": "left",
            "editable": true,
            "insertable": true,
            "visible": true,
            "required": true,
            "defaultValue": "",
            "order": 3
        }
    }
}